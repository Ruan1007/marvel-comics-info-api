{
    "env": {
        "commonjs": true,
        "es2020": true,
        "node": true,
        "jest": true
    },
    "extends": [
        "airbnb-base",
        "eslint-config-prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 11
    },
    "rules": {
        "prettier/prettier": "error",
        "consistent-return": "off",
        "no-console": "off",
        "func-names": "off",
        "prefer-destructuring": "off",
        "no-underscore-dangle": "off",
        "no-unused-expressions": "off",
        "no-restricted-syntax": "off",
        "guard-for-in": "off",
        "no-await-in-loop": "off"
    }
}